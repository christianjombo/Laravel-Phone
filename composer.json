{
    "name": "christianjombo/laravel-phone",
    "description": "This package adds phone number validation functionality to Laravel and Lumen based on Google's libphonenumber API.",
    "keywords": [
        "laravel",
        "lumen",
        "libphonenumber",
        "validation",
        "phone"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "ChristianJombo",
            "email": "ChristianJombo@users.noreply.github.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "illuminate/support": "^5.5|^6.0",
        "illuminate/validation": "^5.5|^6.0",
        "giggsey/libphonenumber-for-php": "^7.0|^8.0",
        "league/iso3166": "^2.0"
    },
    "require-dev": {
        "orchestra/testbench": "*",
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "ChristianJombo\\LaravelPhone\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "ChristianJombo\\LaravelPhone\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ChristianJombo\\LaravelPhone\\PhoneServiceProvider"
            ]
        }
    }
}
